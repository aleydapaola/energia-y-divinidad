{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "prisma generate && next build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "crons": [
    {
      "path": "/api/cron/expire-waitlist-offers",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/cron/expire-credits",
      "schedule": "0 3 * * *"
    }
  ]
}
